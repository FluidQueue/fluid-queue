{
  "name": "fluid-queue",
  "version": "2.0.0-beta.3",
  "description": "A queue system for Super Mario Maker 2 levels",
  "homepage": "https://fluid-queue.dev/",
  "bugs": {
    "url": "https://github.com/fluid-queue/fluid-queue/issues"
  },
  "license": "GPL-3.0",
  "main": "build/index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/fluid-queue/fluid-queue.git"
  },
  "dependencies": {
    "@isaacs/ttlcache": "^1.2.2",
    "@js-joda/core": "^5.5.2",
    "@twurple/api": "^6.1.0",
    "@twurple/auth": "^6.1.0",
    "@twurple/auth-tmi": "^6.1.0",
    "esbuild": "0.17.18",
    "graceful-fs": "^4.2.11",
    "i18next": "^22.4.15",
    "i18next-fs-backend": "^2.1.1",
    "set-interval-async": "^3.0.3",
    "uuid": "^9.0.0",
    "write-file-atomic": "^5.0.0",
    "zod": "^3.21.4"
  },
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --config ./tests/jest.config.json --verbose",
    "start": "node --enable-source-maps ./build/index.js",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\"",
    "build": "node --loader @swc-node/register/esm ./build.ts",
    "clean": "rimraf build/"
  },
  "devDependencies": {
    "@babel/code-frame": "^7.18.6",
    "@swc-node/register": "^1.6.5",
    "@swc/core": "^1.3.55",
    "@swc/jest": "^0.2.26",
    "@tsconfig/node18": "^1.0.3",
    "@types/babel__code-frame": "^7.0.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/tmi.js": "^1.8.3",
    "@types/uuid": "^9.0.1",
    "@types/write-file-atomic": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "assert": "^2.0.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.1.6",
    "glob": "^10.2.2",
    "husky": "^8.0.3",
    "i18next-pseudo": "^2.2.1",
    "jest": "^29.5.0",
    "jest-chance": "^0.1.11",
    "lint-staged": "^13.2.0",
    "memfs": "^3.4.10",
    "prettier": "^2.8.4",
    "readline": "^1.3.0",
    "rimraf": "^5.0.0",
    "tsc-files": "^1.1.3",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": "^18.6.0 || >=19.0.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.ts": "tsc-files --noemit",
    "*.{json,yml,md}": "prettier --write"
  }
}
