{
  "name": "fluid-queue",
  "version": "2.0.0-rc.1",
  "description": "A queue system for Super Mario Maker 2 levels",
  "homepage": "https://fluid-queue.dev/",
  "bugs": {
    "url": "https://github.com/fluid-queue/fluid-queue/issues"
  },
  "license": "GPL-3.0",
  "main": "build/index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/fluid-queue/fluid-queue.git"
  },
  "dependencies": {
    "@isaacs/ttlcache": "^1.4.0",
    "@js-joda/core": "^5.5.3",
    "@twurple/api": "^7.2.0",
    "@twurple/auth": "^7.2.0",
    "@twurple/auth-tmi": "^7.2.0",
    "@twurple/eventsub-base": "^7.2.0",
    "@twurple/eventsub-ws": "^7.2.0",
    "chalk": "^5.4.1",
    "graceful-fs": "^4.2.11",
    "humanize-duration": "^3.28.0",
    "i18next": "^24.2.0",
    "i18next-fs-backend": "^2.6.0",
    "set-interval-async": "^3.0.3",
    "timestring": "^7.0.0",
    "uuid": "^10.0.0",
    "write-file-atomic": "^6.0.0",
    "yaml": "^2.6.1",
    "zod": "^3.24.1"
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config ./tests/jest.config.json --verbose",
    "start": "node --enable-source-maps ./build/index.js",
    "start:swc": "node --import ./register-swc.js ./src/index.ts",
    "prepare": "husky",
    "build": "node --import ./register-swc.js ./build.ts",
    "clean": "rimraf build/",
    "check": "tsc --noemit && eslint .",
    "dist-clean": "rimraf build/ dist/",
    "dist-build": "npm run check && npm run test && npm run build && node --import ./register-swc.js ./dist.ts",
    "fmt": "prettier --write ."
  },
  "devDependencies": {
    "@babel/code-frame": "^7.26.2",
    "@eslint/js": "^9.17.0",
    "@swc-node/register": "^1.10.9",
    "@swc/core": "^1.10.1",
    "@swc/jest": "^0.2.37",
    "@tsconfig/node22": "22.0.0",
    "@types/archiver": "^6.0.3",
    "@types/babel__code-frame": "^7.0.3",
    "@types/chance": "^1.1.3",
    "@types/fs-extra": "^11.0.1",
    "@types/humanize-duration": "^3.27.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/timestring": "^7.0.0",
    "@types/tmi.js": "^1.8.3",
    "@types/uuid": "^10.0.0",
    "@types/write-file-atomic": "^4.0.0",
    "archiver": "^7.0.1",
    "assert": "^2.1.0",
    "esbuild": "^0.24.2",
    "eslint": "~9.9.1",
    "eslint-plugin-jest": "^28.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "fs-extra": "^11.1.1",
    "glob": "^11.0.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "i18next-pseudo": "^2.2.1",
    "jest": "^29.5.0",
    "jest-chance": "^0.2.5",
    "lint-staged": "^15.2.11",
    "memfs": "^4.15.1",
    "prettier": "^3.4.2",
    "readline": "^1.3.0",
    "rimraf": "^6.0.1",
    "tsc-files": "^1.1.3",
    "typescript": "~5.5.4",
    "typescript-eslint": "~8.3.0"
  },
  "engines": {
    "node": ">=22.12.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.3"
  },
  "lint-staged": {
    "*.{js,mjs,cjs,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": "tsc-files --noemit",
    "*.{json,yml,md}": "prettier --write"
  }
}
