{
  "name": "fluid-queue",
  "version": "2.0.0-beta.3",
  "description": "A queue system for Super Mario Maker 2 levels",
  "homepage": "https://fluid-queue.dev/",
  "bugs": {
    "url": "https://github.com/fluid-queue/fluid-queue/issues"
  },
  "license": "GPL-3.0",
  "main": "build/index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/fluid-queue/fluid-queue.git"
  },
  "dependencies": {
    "@isaacs/ttlcache": "^1.4.0",
    "@js-joda/core": "^5.5.3",
    "@twurple/api": "^7.0.0",
    "@twurple/auth": "^7.0.0",
    "@twurple/auth-tmi": "^7.0.0",
    "@twurple/eventsub-base": "^7.0.0",
    "@twurple/eventsub-ws": "^7.0.0",
    "chalk": "^5.3.0",
    "graceful-fs": "^4.2.11",
    "humanize-duration": "^3.28.0",
    "i18next": "^23.2.6",
    "i18next-fs-backend": "^2.1.5",
    "set-interval-async": "^3.0.3",
    "timestring": "^7.0.0",
    "uuid": "^9.0.0",
    "write-file-atomic": "^5.0.1",
    "yaml": "^2.3.0",
    "zod": "^3.21.4"
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config ./tests/jest.config.json --verbose",
    "start": "node --enable-source-maps ./build/index.js",
    "start:swc": "node --loader @swc-node/register/esm ./src/index.ts",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\"",
    "build": "node --loader @swc-node/register/esm ./build.ts",
    "clean": "rimraf build/",
    "check": "tsc --noemit && eslint .",
    "dist-clean": "rimraf build/ dist/",
    "dist-build": "npm run check && npm run test && npm run build && node --loader @swc-node/register/esm ./dist.ts",
    "fmt": "prettier --write ."
  },
  "devDependencies": {
    "@babel/code-frame": "^7.22.0",
    "@swc-node/register": "^1.6.6",
    "@swc/core": "^1.3.67",
    "@swc/jest": "^0.2.26",
    "@tsconfig/node20": "^20.1.2",
    "@types/archiver": "^5.3.2",
    "@types/babel__code-frame": "^7.0.3",
    "@types/chance": "^1.1.3",
    "@types/fs-extra": "^11.0.1",
    "@types/humanize-duration": "^3.27.1",
    "@types/jest": "^29.5.2",
    "@types/node": "~20.9.0",
    "@types/node-fetch": "^2.6.9",
    "@types/timestring": "^6.0.2",
    "@types/tmi.js": "^1.8.3",
    "@types/uuid": "^9.0.2",
    "@types/write-file-atomic": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/typescript-estree": "^6.4.1",
    "archiver": "^6.0.0",
    "assert": "^2.0.0",
    "esbuild": "0.19.2",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.2.3",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.1",
    "husky": "^8.0.3",
    "i18next-pseudo": "^2.2.1",
    "jest": "^29.5.0",
    "jest-chance": "^0.2.5",
    "lint-staged": "^14.0.1",
    "memfs": "^4.2.0",
    "prettier": "^3.0.2",
    "readline": "^1.3.0",
    "rimraf": "^5.0.1",
    "tsc-files": "^1.1.3",
    "typescript": "~5.1.6"
  },
  "engines": {
    "node": ">=20.9.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.3"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.ts": "tsc-files --noemit",
    "*.{json,yml,md}": "prettier --write"
  }
}
